{
  "name": "rn-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "android": "react-native run-android --mode=developmentDebug --appIdSuffix 'dev'",
    "android:release": "cd android && ./gradlew assembleProductionRelease",
    "ios": "react-native run-ios --scheme RNTemplate --mode Dev.Debug",
    "lint": "eslint . --cache",
    "lint:fix": "eslint --fix .",
    "start": "ENVFILE=.env.development react-native start",
    "test": "jest --colors --config ./jest.config.js",
    "mock-server": "yarn workspace mock-server start",
    "prettier:check": "prettier --check './**/*.{ts,tsx,js,jsx}'",
    "prettier:fix": "prettier --write './**/*.{ts,tsx,js,jsx}'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.18.1",
    "@react-native-picker/picker": "2.4.10",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/native-stack": "6.9.12",
    "@rneui/base": "4.0.0-rc.7",
    "@rneui/themed": "4.0.0-rc.7",
    "@xstate/react": "3.2.2",
    "axios": "1.4.0",
    "axios-mock-adapter": "1.21.4",
    "i18next": "22.4.15",
    "react": "18.2.0",
    "react-i18next": "12.2.2",
    "react-native": "0.71.7",
    "react-native-config": "^1.5.0",
    "react-native-flipper": "0.190.0",
    "react-native-keychain": "8.1.1",
    "react-native-safe-area-context": "4.5.1",
    "react-native-screens": "^3.20.0",
    "react-native-splash-screen": "3.3.0",
    "react-native-vector-icons": "9.2.0",
    "react-query": "3.39.3",
    "xstate": "4.37.2"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "7.21.0",
    "@babel/preset-env": "7.21.4",
    "@babel/runtime": "7.21.0",
    "@testing-library/jest-native": "5.4.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "12.0.1",
    "@tsconfig/react-native": "3.0.0",
    "@types/jest": "^29.5.1",
    "@types/react": "18.2.0",
    "@types/react-native": "0.71.6",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "commitizen": "4.3.0",
    "cz-format-extension": "1.5.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-import-resolver-typescript": "3.4.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "8.0.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "metro-react-native-babel-preset": "0.70.3",
    "obfuscator-io-metro-plugin": "2.1.1",
    "prettier": "2.7.1",
    "react-native-flipper-xstate": "1.0.5",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.8.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-format-extension"
    }
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
}
